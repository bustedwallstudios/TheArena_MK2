; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

[application]

config/name="The Arena"
config/description="MOELST"
run/main_scene="res://scenes/MainMenu.tscn"
config/icon="res://icon.png"

[autoload]

MyTimer="*res://script/utilities/myTimer.gd"
GlobalScore="*res://script/GlobalScore.gd"
Global="*res://script/utilities/global.gd"

[debug]

settings/fps/force_fps=60
settings/stdout/print_fps=true

[display]

window/size/width=1280
window/size/height=720
window/handheld/orientation="sensor_landscape"
window/stretch/mode="2d"
window/stretch/aspect="keep"

[global]

fps=false
force_fps=false

[gui]

common/drop_mouse_on_gui_input_disabled=true

[input_devices]

pointing/emulate_touch_from_mouse=true

[physics]

common/physics_fps=144
common/enable_pause_aware_picking=true

[rendering]

environment/default_clear_color=Color( 0.0666667, 0.0666667, 0.0666667, 1 )
quality/depth/hdr.mobile=true
environment/default_environment="res://default_env.tres"
